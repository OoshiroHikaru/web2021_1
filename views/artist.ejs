<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>アーティスト一覧</title>
   <style>
    body {
      background-color: #000000;
      color: #ffffff;
      font-family: Arial, sans-serif;
    }
    table {
      width: 100%;
      border-collapse: collapse;
    }
    th, td {
      border: 1px solid #ffffff;
      padding: 10px;
      text-align: left;
    }
    th {
      background-color: #333333;
    }
    tr:nth-child(even) {
      background-color: ##333333;
    }
    a {
      color: #ff0000;
      text-decoration: none;
    }
    a:hover {
      text-decoration: underline;
    }
  </style>
</head>
<body>
  <h2>アーティスト一覧</h2>
  <table border="1">
    <tr>
      <th>ID</th>
      <th>アーティスト名</th>
    </tr>
    <% data.forEach(function(row) { %>
      <tr id="artist<%= row.id %>">
        <td><%= row.id %></td>
        <td>
          <a href="/artist/<%= row.id %>"><%= row.artist_name %></a>
        </td>
      </tr>
    <% }); %>
  </table>
</body>
</html>


